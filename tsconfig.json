{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "#components/*": ["./src/components/*"],
      "#containers/*": ["./src/containers/*"],
      "#contexts/*": ["./src/contexts/*"],
      "#env": ["./src/env.js"],
      "#helpers/*": ["./src/helpers/*"],
      "#hoc/*": ["./src/hoc/*"],
      "#hooks/*": ["./src/hooks/*"],
      "#images/*": ["./src/images/*"],
      "#mocks/*": ["./src/mocks/*"],
      "#modules/*": ["./src/modules/*"],
      "#services/*": ["./src/services/*"],
      "#theme": ["./src/Theme.ts"],
      "#types": ["./src/types.ts"],
      "#utils/*": ["./src/utils/*"],
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}